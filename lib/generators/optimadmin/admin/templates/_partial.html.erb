<div class="row collapse module-list-index <%%= cycle('odd', 'even') %>" data-equalizer="module-list" id="item_<%%= <%= singular_table_name %>_presenter.id %>">
  <div class="large-1 small-2 columns" data-equalizer-watch="module-list">
    <%%= <%= singular_table_name %>_presenter.detail_toggle_link %>
  </div>

  <div class="large-5 small-6 columns" data-equalizer-watch="module-list">
    <%%= <%= singular_table_name %>_presenter.title %>
  </div>

  <% unless attributes.select{|x| x.type == :image }.size.zero? %>
    <div class="large-2 hide-for-medium-down columns" data-equalizer-watch="module-list">
      <%%= it_edit_images(:image).call(page) %>
    </div>
  <% end %>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%%= <%= singular_table_name %>_presenter.toggle_link %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%%= <%= singular_table_name %>_presenter.show_link %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%%= <%= singular_table_name %>_presenter.edit_link %>
  </div>

  <div class="large-1 hide-for-medium-down columns last" data-equalizer-watch="module-list">
    <%%= <%= singular_table_name %>_presenter.delete_link %>
  </div>

  <div class="row module-list-detail hide" id="index-list-<%%= <%= singular_table_name %>_presenter.id %>">
    <div class="small-12 columns module-list-summary last">

    </div>
  </div>
</div>
